{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/app.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/client/build/index.html"
    }
  ],
  "functions": {
    "/server/app.js": {
      "runtime": "vercel-node@18.x"
    }
  }
}